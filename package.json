{
  "name": "vue-nuxt-magazines",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "prepare": "husky install",
    "lint": "yarn lint:js && yarn lint:style",
    "heroku-postbuild": "yarn run build && yarn run generate"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/i18n": "^7.1.0",
    "animate.css": "^4.1.1",
    "client-zip": "^2.0.0",
    "core-js": "^3.15.1",
    "file-saver": "^2.0.5",
    "nuxt": "^2.15.7",
    "pdfjs-dist": "2.3.200",
    "sass": "^1.39.2",
    "vue-pdf": "^4.3.0",
    "vue-scroll-snap": "^0.1.11",
    "vue-social-sharing": "^3.0.8"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@commitlint/cli": "^12.1.4",
